[Unit]
Description=PostgreSQL
Requires=docker-registry.service
After=docker-registry.service

[Service]
TimeoutStartSec=0
RemainAfterExit=yes
Environment='POSTGRES_NAME=postgres'
ExecStartPre=/bin/sh /opt/data/fleet/postgres/start-pre.sh
ExecStart=/bin/sh /opt/data/fleet/postgres/start.sh
ExecStop=/bin/sh /opt/data/fleet/postgres/stop.sh
